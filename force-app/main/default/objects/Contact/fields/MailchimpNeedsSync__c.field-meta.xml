<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MailchimpNeedsSync__c</fullName>
    <description>Whether the record has been updated since it was last synced to Mailchimp.</description>
    <externalId>false</externalId>
    <formula>NOT(ISBLANK(MailchimpLastChanged__c)) &amp;&amp; (ISBLANK(MailchimpLastSyncedAt__c) || MailchimpLastChanged__c - MailchimpLastSyncedAt__c &gt; 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Needs sync to Mailchimp?</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
